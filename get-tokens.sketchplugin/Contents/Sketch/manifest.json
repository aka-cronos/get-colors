{
  "name": "Get Tokens",
  "description": "Generate JSON tokens files from styles",
  "$schema": "https://raw.githubusercontent.com/sketch-hq/SketchAPI/develop/docs/sketch-plugin-manifest-schema.json",
  "icon": "icon.png",
  "commands": [
    {
      "name": "Get Colors",
      "identifier": "getTokens.getColors",
      "script": "__get-color-tokens.js",
      "description": "Get color variables"
    },
    {
      "name": "Get Text",
      "identifier": "getTokens.getText",
      "script": "__get-text-tokens.js",
      "description": "Get text styles"
    }
  ],
  "menu": {
    "title": "Get Tokens",
    "items": [
      "getTokens.getColors",
      "-",
      "getTokens.getText"
    ]
  },
  "version": "0.1.0",
  "identifier": "get-tokens",
  "disableCocoaScriptPreprocessor": true,
  "appcast": "https://raw.githubusercontent.com//master/.appcast.xml",
  "author": "Cronos",
  "authorEmail": "jorge.escamilla89@gmail.com"
}